{
  "extends": [
    "airbnb",
    "airbnb/hooks"
  ],
  "plugins": [
    "react"
  ],
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "no-restricted-imports": [
      "error",
      {
        "patterns": [
          "@material-ui/*/*/*"
        ]
      }
    ],
    "quotes": [
      "error",
      "single",
      {
        "allowTemplateLiterals": true
      }
    ],
    "jsx-quotes": [2, "prefer-single"],
    "comma-dangle": [
      "error",
      "always-multiline"
    ],
    "react/prop-types": 0
  }
}
